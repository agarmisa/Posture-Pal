<div class="admin-photo-container">
  <div class="row">
    <div class="columns small-6 left-text">
      <ul class="admin-list">
        <strong>Administrators:</strong>
        <% @admins.each do |admin| %>
          <li><%= admin.name%> | <%= admin.email%></li>
        <% end %>
      </ul>
        <hr>

      <ul class="member-list">
        <strong>Members:</strong>
        <% @members.each do |member| %>
          <li><%= member.name%> | <%= member.email%></li>
          <li><%= link_to "Delete This User", user_path(member), method: :delete %></li>
          <br>
        <% end %>
      </ul>
    </div>
  </div>
</div>
